# Velociraptor version (tag from https://github.com/Velocidex/velociraptor/releases)
VELOCIRAPTOR_TAG=v0.75
VELOCIRAPTOR_VERSION=v0.75.1

# Hostname or IP the client should reach (inside compose network use service name)
SERVER_HOSTNAME=velociraptor-server

# Exposed ports on the host
GUI_PORT=8889
API_PORT=8001

# Initial admin credentials
ADMIN_USER=admin
ADMIN_PASSWORD=admin

# API client identity to generate mTLS credentials for
API_USER=api-admin
